[mysqld]
server-id = 1
expire_logs_days                = 7
auto_increment_offset           = 1
auto_increment_increment        = 2
innodb_flush_log_at_trx_commit = 1
log-bin = /data/mysqld-bin.log
relay-log = /data/mysqld-relay-bin.log
log_slave_updates
slave_skip_errors               = OFF

